{
  "manifest_version": 3,
  "name": "ReadUnlocked",
  "version": "1.0",
  "description": "Bypass paywalls and read articles for free.",
  "permissions": ["tabs"],
  "action": {
    "default_title": "Read articles for Free",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "Qalib Q.",
  "homepage_url": "https://github.com/qalibqurbanov"
}
